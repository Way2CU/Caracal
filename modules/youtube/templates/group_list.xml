<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<div class="main_menu">
		<cms:var name="$params['link_new']"/>
	</div>

	<div class="scrollable_list">
		<div class="list_header">
			<span class="column" style="width: 300px;">
				<cms:text constant="column_name" module="youtube"/>
			</span>
			<span class="column" style="width: 60px; text-align: center;">
				<cms:text constant="column_visible" module="youtube"/>
			</span>
			<span class="column">
				<cms:text constant="column_options"/>
			</span>
		</div>
		<div class="list_content" style="height: 250px">
			<_group_list/>
		</div>
	</div>

</document>
