<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<div class="list_item">
		<span class="column" style="width: 200px;">
			<cms:var name="$params['fullname']"/>
		</span>
		<span class="column" style="width: 150px;">
			<cms:var name="$params['username']"/>
		</span>
		<span class="column" style="width: 50px; text-align: center;">
			<cms:var name="$params['level']"/>
		</span>
		<span class="column" style="width: 60px; text-align: center;">
			<cms:var name="$params['verified_char']"/>
		</span>
		<span class="column" style="width: 60px; text-align: center;">
			<cms:var name="$params['agreed_char']"/>
		</span>
		<span class="column">
			<cms:var name="$params['item_change']"/><cms:raw text=" "/>
			<cms:var name="$params['item_delete']"/>
		</span>
	</div>
</document>
